<?xml version="1.0" encoding="UTF-8"?>
<persistence version="3.0" xmlns="https://jakarta.ee/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd">
  <!-- Define Persistence Unit -->
  <persistence-unit name="default" transaction-type="JTA">
    <jta-data-source>java:app/Prod</jta-data-source>
    <class>com.mycompany.mavenproject1.model.Game</class>
    <class>com.mycompany.mavenproject1.model.Results</class>
    <class>com.mycompany.mavenproject1.model.Team</class>
    <class>com.mycompany.mavenproject1.model.User</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
  </persistence-unit>
</persistence>
